@book{knuth,
title 		= {The art of computer programming},
author 		= {Knuth, Donald Ervin},
publisher 	= {Addison-Wesley},
volume 		= "1-Fundamental algorithms, 2-Seminumerical Algorithms, 3-Searching and sorting",
year 		= 1997,
note 		= {\mbox{}\\{\footnotesize « La » référence en algorithmique. Deux autres volumes sont en préparation :
			4-Combinatorial Algorithms et 5-Syntactic Algorithms. Pour la petite histoire, D.E. Knuth est également
			l'auteur de \TeX\ (prononcer tech comme dans technologie), système logiciel de composition de documents --- utilisé pour générer le document que vous êtes
			en train de lire --- créé à partir de 1977 pour répondre à la mauvaise qualité typographique de la deuxième édition 
			de {\em The art of computer programming}.\\
			cité pages \pageref{cite:knuth1},\pageref{cite:knuth2}}}
}

