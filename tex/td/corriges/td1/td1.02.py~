# -*- coding: utf-8 -*-
 
# avec la tortue Logo
from turtle import *

# dessin du 1er côté
forward(10)

# tourner à gauche de 90°
left(90)

# dessin du 2ème côté
forward(10+2)

# tourner à gauche de 90°
left(90)

# dessin du 3ème côté
forward(10+2+2)

# tourner à gauche de 90°
left(90)

# dessin du 4ème côté
forward(10+2+2+2)

# tourner à gauche de 90°
left(90)

# dessin du 5ème côté
forward(10+2+2+2+2)

# tourner à gauche de 90°
left(90)


