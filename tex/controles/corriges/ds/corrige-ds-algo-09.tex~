%-----------------------------------------------------------------------------
\section{Exécution d'une fonction de tri}
%\lstinputlisting[basicstyle=\footnotesize]{ds-algo-09-tri.py}

\begin{py}{8cm}
\begin{verbatim}
>>> f([5,1,4,7,10])
0 [10, 1, 4, 7, 5]
1 [10, 7, 4, 1, 5]
2 [10, 7, 5, 1, 4]
3 [10, 7, 5, 4, 1]
4 [10, 7, 5, 4, 1]
\end{verbatim}
\end{py}

%-----------------------------------------------------------------------------
\section{Exécution d'une fonction de sélection}
%\lstinputlisting[basicstyle=\footnotesize]{ds-algo-09-selection.py}

\begin{py}{8cm}
\begin{verbatim}
>>> g(h,[5,1,4,7,10])
1 [5, 1, 4, 7, 10]
2 [5, 1, 4, 7, 10]
2 [5, 1, 7, 10]
3 [5, 1, 7, 10]
3 [5, 1, 7]
\end{verbatim}
\end{py}

